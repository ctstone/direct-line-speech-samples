{
  "name": "bot-streaming-express",
  "version": "1.0.0",
  "description": "Sample Node.js streaming server for Bot Framework",
  "main": "index.js",
  "scripts": {
    "start": "node dist",
    "prepare": "npm run build",
    "build": "tsc",
    "start-dev": "node -r dotenv/config dist",
    "watch": "tsc-watch --onSuccess \"npm run start-dev\"",
    "publish-luis": "node -r dotenv/config scripts/publish-luis",
    "refresh-luis": "node -r dotenv/config scripts/refresh-luis",
    "test": "TS_NODE_PROJECT=./test/tsconfig.json mocha -r ts-node/register ./test/*.spec.ts",
    "cov": "nyc npm test"
  },
  "keywords": [],
  "author": "chstone@microsoft.com",
  "license": "MIT",
  "dependencies": {
    "@types/mocha": "^5.2.7",
    "axios": "^0.21.1",
    "botbuilder": "^4.4.0",
    "botbuilder-ai": "^4.4.0",
    "dark-sky": "^1.1.5",
    "express": "^4.17.1",
    "microsoft-bot-protocol": "file:lib/microsoft-bot-protocol",
    "microsoft-bot-protocol-namedpipe": "file:lib/microsoft-bot-protocol-namedpipe",
    "microsoft-bot-protocol-streamingextensions": "file:lib/microsoft-bot-protocol-streamingextensions",
    "microsoft-bot-protocol-websocket": "file:lib/microsoft-bot-protocol-websocket",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "y18n": "^4.0.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/express": "^4.17.0",
    "@types/moment-timezone": "^0.5.12",
    "axios-mock-adapter": "^1.16.0",
    "chai": "^4.2.0",
    "chai-bignumber": "^3.0.0",
    "chai-string": "^1.5.0",
    "coveralls": "^3.0.4",
    "dotenv": "^8.0.0",
    "ludown": "^1.3.1",
    "luis-apis": "^2.5.1",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "mockdate": "^2.0.3",
    "ms-rest-js": "^1.0.1",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0",
    "tsc-watch": "^2.2.1",
    "tslint": "^5.17.0",
    "typescript": "^3.5.2"
  }
}
